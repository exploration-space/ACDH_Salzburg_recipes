             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="pfanne">Pfanne</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="stollhafen">Stollhafen</object>
			<object type="MaterielleKultur" xml:id="topf-2">Topf</object>
                </listObject>
