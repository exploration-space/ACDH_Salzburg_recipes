        <body>
            <div type="ingredient">
                <listObject>
			<object type="Zutaten" xml:id="aepfel">Äpfel</object>
			<object type="Zutaten" xml:id="eier">Eier</object>
			<object type="Zutaten" xml:id="eiklar">Eiklar (Eiweiß)</object>
