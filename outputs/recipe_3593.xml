			<object type="MaterielleKultur" xml:id="rost">Rost</object>
			<object type="MaterielleKultur" xml:id="stein">Stein</object>
			<object type="MaterielleKultur" xml:id="topf-2">Topf</object>
			<object type="MaterielleKultur" xml:id="topf-hafen-glasiert">Topf (Hafen - glasiert)</object>
			<object type="MaterielleKultur" xml:id="tuch-weiss">Tuch (weiß)</object>
