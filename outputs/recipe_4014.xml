             <div type="utensil">
                <listObject>
			<object type="utensil" xml:id="faden">Faden</object>
			<object type="utensil" xml:id="kochloeffel">Kochlöffel</object>
			<object type="utensil" xml:id="schmalzpfaennlein">Schmalzpfännlein</object>
