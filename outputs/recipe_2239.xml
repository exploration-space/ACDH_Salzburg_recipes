			<object type="utensil" xml:id="pfanne">Pfanne</object>
			<object type="utensil" xml:id="schaumloeffel">Schaumlöffel</object>
			<object type="utensil" xml:id="schuessel">Schüssel</object>
			<object type="utensil" xml:id="straubenloeffel">Straubenlöffel</object>
			<object type="utensil" xml:id="tontopf-rein">Tontopf (Rein)</object>
