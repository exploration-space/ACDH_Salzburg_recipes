			<object type="MaterielleKultur" xml:id="holz">Holz</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="spicknadel-aus-holz">Spicknadel (aus Holz)</object>
