             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="feuer">Feuer</object>
			<object type="MaterielleKultur" xml:id="schale">Schale</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="hafen-topf">Topf (Hafen)</object>
