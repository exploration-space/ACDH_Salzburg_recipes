        <body>
            <div type="ingredient">
                <listObject>
			<object type="Zutaten" xml:id="feigen">Feigen</object>
			<object type="Zutaten" xml:id="gewuerze">Gewürze</object>
			<object type="Zutaten" xml:id="mehl">Mehl</object>
