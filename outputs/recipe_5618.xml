             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="waffel-eisen">(Waffel-)Eisen</object>
			<object type="MaterielleKultur" xml:id="dreifuss">Dreifuß</object>
			<object type="MaterielleKultur" xml:id="glashafen">Glashafen</object>
			<object type="MaterielleKultur" xml:id="loeffel">Löffel</object>
			<object type="MaterielleKultur" xml:id="tuch">Tuch</object>
