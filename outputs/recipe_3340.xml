        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="brot">Brot</object>
			<object type="Zutaten" xml:id="butter">Butter</object>
			<object type="Zutaten" xml:id="essig">Essig</object>
			<object type="Zutaten" xml:id="huhn">Huhn</object>
