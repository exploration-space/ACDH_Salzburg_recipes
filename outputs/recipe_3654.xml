        <body>
            <div type="ingredient">
                <listObject>
			<object type="ingredient" xml:id="butter">Butter</object>
			<object type="ingredient" xml:id="eier">Eier</object>
			<object type="ingredient" xml:id="eierbrot">Eierbrot</object>
			<object type="ingredient" xml:id="fleischbruehe">Fleischbrühe</object>
			<object type="ingredient" xml:id="muskatbluete">Muskatblüte</object>
