        <body>
            <div type="ingredient">
                <listObject>
			<object type="ingredient" xml:id="butter">Butter</object>
			<object type="ingredient" xml:id="kalbsfaschiertes">Kalbsfaschiertes</object>
			<object type="ingredient" xml:id="kalbskotelett">Kalbskotelett</object>
