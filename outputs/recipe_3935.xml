			<object type="Zutaten" xml:id="rindfleisch">Rindfleisch</object>
			<object type="Zutaten" xml:id="schmalz">Schmalz</object>
                 </listObject>
            </div>
             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="brett">Brett</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="sieb">Sieb</object>
			<object type="MaterielleKultur" xml:id="topf">Topf (Rein)</object>
                </listObject>
