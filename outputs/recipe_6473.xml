			<object type="Zutaten" xml:id="salz">Salz</object>
			<object type="Zutaten" xml:id="zucker">Zucker</object>
                 </listObject>
            </div>
             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="holzloeffel">Holzlöffel</object>
			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
			<object type="MaterielleKultur" xml:id="papier">Papier</object>
                </listObject>
