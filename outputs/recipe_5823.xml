        <body>
            <div type="ingredient">
                <listObject>
			<object type="ingredient" xml:id="butter">Butter</object>
			<object type="ingredient" xml:id="erbsensuppe">Erbsenbrühe</object>
			<object type="ingredient" xml:id="essig">Essig</object>
			<object type="ingredient" xml:id="hausen-fisch">Hausen (Fisch)</object>
