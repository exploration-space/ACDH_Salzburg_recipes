        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="amseln">Amseln</object>
			<object type="Zutaten" xml:id="butter">Butter</object>
			<object type="Zutaten" xml:id="drosseln">Drosseln</object>
			<object type="Zutaten" xml:id="fleischbruehe">Fleischbrühe</object>
			<object type="Zutaten" xml:id="mehl-braun">Mehl (braun)</object>
			<object type="Zutaten" xml:id="salz">Salz</object>
			<object type="Zutaten" xml:id="semmelbroesel">Semmelbrösel</object>
			<object type="Zutaten" xml:id="wacholderbeeren">Wacholderbeeren</object>
