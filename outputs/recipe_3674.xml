			<object type="Zutaten" xml:id="zitronen">Zitronen</object>
			<object type="Zutaten" xml:id="zucker">Zucker</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="feuer">Feuer</object>
			<object type="MaterielleKultur" xml:id="moerser">Mörser</object>
			<object type="MaterielleKultur" xml:id="pastetenofen">Pastetenofen</object>
			<object type="MaterielleKultur" xml:id="seitlein">Seitlein</object>
			<object type="MaterielleKultur" xml:id="sieb">Sieb</object>
			<object type="MaterielleKultur" xml:id="tortenpfanne">Tortenpfanne</object>
			<object type="MaterielleKultur" xml:id="zinnschuessel">Zinnschüssel</object>
                </listObject>
