			<object type="MaterielleKultur" xml:id="blech-aus-zinn">Blech (aus Zinn)</object>
			<object type="MaterielleKultur" xml:id="blechmodel-ohne-boden">Blechmodel (ohne Boden)</object>
			<object type="MaterielleKultur" xml:id="glasschale">Glasschale</object>
			<object type="MaterielleKultur" xml:id="marmorstein-platte">Marmorstein (Platte)</object>
			<object type="MaterielleKultur" xml:id="messingbecken-becken">Messingbecken</object>
			<object type="MaterielleKultur" xml:id="schachtel">Schachtel</object>
