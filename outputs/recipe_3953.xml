			<object type="Zutaten" xml:id="schmalz">Schmalz</object>
			<object type="Zutaten" xml:id="zucker">Zucker</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="loeffel">Löffel</object>
			<object type="MaterielleKultur" xml:id="loth">Loth</object>
			<object type="MaterielleKultur" xml:id="nudelwalker-nudelholz">Nudelwalker (Nudelholz)</object>
			<object type="MaterielleKultur" xml:id="trichter">Trichter</object>
                </listObject>
