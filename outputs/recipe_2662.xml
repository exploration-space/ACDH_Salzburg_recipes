			<object type="Zutaten" xml:id="semmelrinde">Semmelrinde</object>
			<object type="Zutaten" xml:id="taube">Taube</object>
			<object type="Zutaten" xml:id="zucker-gefaeht">Zucker (gefäht)</object>
			<object type="Zutaten" xml:id="zwiebel">Zwiebel</object>
                 </listObject>
            </div>
             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="haarsieb">Haarsieb</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="topf">Topf (Rein)</object>
                </listObject>
