			<object type="Zutaten" xml:id="zucker">Zucker</object>
                 </listObject>
            </div>
             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="durchschlag">Durchschlag</object>
			<object type="MaterielleKultur" xml:id="loeffel">Löffel</object>
			<object type="MaterielleKultur" xml:id="moerser">Mörser</object>
			<object type="MaterielleKultur" xml:id="tuch">Tuch</object>
                </listObject>
