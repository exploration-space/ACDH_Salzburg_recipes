             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="bratpfanne">Bratpfanne</object>
			<object type="MaterielleKultur" xml:id="feuer">Feuer</object>
			<object type="MaterielleKultur" xml:id="fingerhut">Fingerhut</object>
			<object type="MaterielleKultur" xml:id="kohlen">Kohlen</object>
			<object type="MaterielleKultur" xml:id="maas">Maas</object>
			<object type="MaterielleKultur" xml:id="messer">Messer</object>
			<object type="MaterielleKultur" xml:id="pfund">Pfund</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="spiess">Spieß</object>
                </listObject>
