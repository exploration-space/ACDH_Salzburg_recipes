                <listObject>
			<object type="Zutaten" xml:id="eier">Eier</object>
			<object type="Zutaten" xml:id="mandeln">Mandeln</object>
			<object type="Zutaten" xml:id="mehl">Mehl</object>
			<object type="Zutaten" xml:id="nelken-gem">Nelken (gem.)</object>
			<object type="Zutaten" xml:id="staerkezucker-traubenzucker">Stärkezucker (Traubenzucker)</object>
