			<object type="Zutaten" xml:id="zitronensaft">Zitronensaft</object>
			<object type="Zutaten" xml:id="zwiebel">Zwiebel</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="topf">Topf (Rein)</object>
                </listObject>
