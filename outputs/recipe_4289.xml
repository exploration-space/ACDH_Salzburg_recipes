             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="messer">Messer</object>
			<object type="MaterielleKultur" xml:id="moerser">Mörser</object>
			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
			<object type="MaterielleKultur" xml:id="tortenschuessel">Tortenschüssel</object>
                </listObject>
