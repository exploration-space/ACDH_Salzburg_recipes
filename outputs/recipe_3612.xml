        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="butter">Butter</object>
			<object type="Zutaten" xml:id="fleischbruehe">Fleischbrühe</object>
			<object type="Zutaten" xml:id="gewuerze">Gewürze</object>
			<object type="Zutaten" xml:id="hausen-fisch">Hausen (Fisch)</object>
			<object type="Zutaten" xml:id="kraeuter">Kräuter</object>
			<object type="Zutaten" xml:id="limonensaft">Limonensaft</object>
			<object type="Zutaten" xml:id="mehl">Mehl</object>
			<object type="Zutaten" xml:id="salz">Salz</object>
