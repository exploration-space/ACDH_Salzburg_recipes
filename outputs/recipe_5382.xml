			<object type="MaterielleKultur" xml:id="eisenloeffel">Eisenlöffel</object>
			<object type="MaterielleKultur" xml:id="kohlpfanne">Kohlpfanne</object>
			<object type="MaterielleKultur" xml:id="loeffel">Löffel</object>
			<object type="MaterielleKultur" xml:id="pfaennlein">Pfännlein</object>
