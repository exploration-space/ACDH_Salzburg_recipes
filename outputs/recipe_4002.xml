             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="brett">Brett</object>
			<object type="MaterielleKultur" xml:id="geschirr">Geschirr</object>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
