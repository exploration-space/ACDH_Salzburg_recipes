             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
			<object type="MaterielleKultur" xml:id="papier">Papier</object>
			<object type="MaterielleKultur" xml:id="reibeisen">Reibeisen</object>
			<object type="MaterielleKultur" xml:id="spiess">Spieß</object>
                </listObject>
