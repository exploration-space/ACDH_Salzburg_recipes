        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="dotter">Dotter (Eigelb)</object>
			<object type="Zutaten" xml:id="eier">Eier</object>
			<object type="Zutaten" xml:id="fleischbruehe">Fleischbrühe</object>
			<object type="Zutaten" xml:id="muskatbluete">Muskatblüte</object>
			<object type="Zutaten" xml:id="roggenbrot-brot">Roggenbrot</object>
