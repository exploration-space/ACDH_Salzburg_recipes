             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="messer">Messer</object>
			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
