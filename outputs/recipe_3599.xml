			<object type="Zutaten" xml:id="pfeffer">Pfeffer</object>
			<object type="Zutaten" xml:id="rosmarin">Rosmarin</object>
			<object type="Zutaten" xml:id="zwiebel">Zwiebel</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="kasserolle">Kasserolle</object>
			<object type="MaterielleKultur" xml:id="schuesselring">Schüsselring (Reif)</object>
			<object type="MaterielleKultur" xml:id="topf">Topf (Rein)</object>
                </listObject>
