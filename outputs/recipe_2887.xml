        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="essig">Essig</object>
			<object type="Zutaten" xml:id="fleischbruehe">Fleischbrühe</object>
			<object type="Zutaten" xml:id="kapaun">Kapaun</object>
