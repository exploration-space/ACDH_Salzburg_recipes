        <body>
            <div type="ingredient">
                <listObject>
			<object type="ingredient" xml:id="butter">Butter</object>
			<object type="ingredient" xml:id="eier">Eier</object>
			<object type="ingredient" xml:id="muskatbluete">Muskatblüte</object>
			<object type="ingredient" xml:id="sardellen">Sardellen</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="utensil" xml:id="schuessel">Schüssel</object>
			<object type="utensil" xml:id="hafen-topf">Topf (Hafen)</object>
			<object type="utensil" xml:id="zinnschuessel">Zinnschüssel</object>
                </listObject>
