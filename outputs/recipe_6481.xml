        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="mehl">Mehl</object>
			<object type="Zutaten" xml:id="rosenwasser">Rosenwasser</object>
			<object type="Zutaten" xml:id="tragant-weiss">Tragant (weiß)</object>
			<object type="Zutaten" xml:id="zucker">Zucker</object>
                 </listObject>
            </div>
             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="haartuch">Haartuch</object>
			<object type="MaterielleKultur" xml:id="moerser">Mörser</object>
			<object type="MaterielleKultur" xml:id="nudelwalker-nudelholz">Nudelwalker (Nudelholz)</object>
			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
