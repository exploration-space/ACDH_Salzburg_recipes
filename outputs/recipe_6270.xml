        <body>
            <div type="ingredient">
                <listObject>
			<object type="Zutaten" xml:id="butter">Butter</object>
			<object type="Zutaten" xml:id="eier">Eier</object>
			<object type="Zutaten" xml:id="kraeuter-frisch">Kräuter (frisch)</object>
			<object type="Zutaten" xml:id="pfeffer">Pfeffer</object>
			<object type="Zutaten" xml:id="rinderfett">Rinderfett</object>
			<object type="Zutaten" xml:id="safran">Safran</object>
			<object type="Zutaten" xml:id="schweinsnetz">Schweinsnetz</object>
