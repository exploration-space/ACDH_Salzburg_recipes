             <div type="utensil">
                <listObject>
			<object type="utensil" xml:id="loeffel">Löffel</object>
			<object type="utensil" xml:id="maas">Maas</object>
			<object type="utensil" xml:id="moerser-aus-gestein">Mörser (aus Gestein)</object>
			<object type="utensil" xml:id="pfanne">Pfanne</object>
			<object type="utensil" xml:id="reibeschale">Reibeschale</object>
