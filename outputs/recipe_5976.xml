             <div type="utensil">
                <listObject>
			<object type="utensil" xml:id="glut">Glut</object>
			<object type="utensil" xml:id="pfanne">Pfanne</object>
			<object type="utensil" xml:id="schuessel">Schüssel</object>
			<object type="utensil" xml:id="spiess">Spieß</object>
                </listObject>
