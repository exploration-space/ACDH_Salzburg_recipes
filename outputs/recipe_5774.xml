        <body>
            <div type="ingredient">
                <listObject>
			<object type="Zutaten" xml:id="butter">Butter</object>
			<object type="Zutaten" xml:id="dotter">Dotter (Eigelb)</object>
			<object type="Zutaten" xml:id="kalbfleisch">Kalbfleisch</object>
			<object type="Zutaten" xml:id="mehl">Mehl</object>
			<object type="Zutaten" xml:id="olivenoel">Olivenöl</object>
			<object type="Zutaten" xml:id="pastete">Pastete</object>
			<object type="Zutaten" xml:id="rinderbruehe">Rinderbrühe</object>
			<object type="Zutaten" xml:id="schweineschmalz">Schweineschmalz</object>
			<object type="Zutaten" xml:id="wasser">Wasser</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="messer">Messer</object>
			<object type="MaterielleKultur" xml:id="nudelwalker-nudelholz">Nudelwalker (Nudelholz)</object>
			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
			<object type="MaterielleKultur" xml:id="pinsel">Pinsel</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
