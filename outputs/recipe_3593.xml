             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="bratpfanne">Bratpfanne</object>
			<object type="MaterielleKultur" xml:id="deckel">Deckel</object>
			<object type="MaterielleKultur" xml:id="kanne">Kanne</object>
