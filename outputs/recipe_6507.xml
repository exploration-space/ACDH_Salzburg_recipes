             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="kasserolle">Kasserolle</object>
			<object type="MaterielleKultur" xml:id="pfanne">Pfanne</object>
			<object type="MaterielleKultur" xml:id="teller">Teller</object>
