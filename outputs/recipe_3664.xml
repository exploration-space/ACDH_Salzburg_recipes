             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="deckel-fuer-glut-stuerze">Deckel für Glut (Stürze)</object>
			<object type="MaterielleKultur" xml:id="dreifuss">Dreifuß</object>
			<object type="MaterielleKultur" xml:id="kohlen">Kohlen</object>
