			<object type="Zutaten" xml:id="nelken">Nelken</object>
			<object type="Zutaten" xml:id="pfeffer">Pfeffer</object>
			<object type="Zutaten" xml:id="zwiebel">Zwiebel</object>
                 </listObject>
            </div>
             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="geschirr">Geschirr</object>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="haarsieb">Haarsieb</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
                </listObject>
