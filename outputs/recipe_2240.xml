			<object type="utensil" xml:id="becken">Becken</object>
			<object type="utensil" xml:id="deckel">Deckel</object>
			<object type="utensil" xml:id="schuessel">Schüssel</object>
			<object type="utensil" xml:id="schuesselring">Schüsselring (Reif)</object>
                </listObject>
