        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="artischocken">Artischocken</object>
			<object type="Zutaten" xml:id="oel">Öl</object>
			<object type="Zutaten" xml:id="pfeffer">Pfeffer</object>
			<object type="Zutaten" xml:id="salz">Salz</object>
