			<object type="Zutaten" xml:id="kutteln">Kutteln (Kaldaunen)</object>
			<object type="Zutaten" xml:id="lorbeer">Lorbeerblätter</object>
			<object type="Zutaten" xml:id="petersilie">Petersilie</object>
			<object type="Zutaten" xml:id="rosmarin">Rosmarin</object>
			<object type="Zutaten" xml:id="semmeln">Semmeln</object>
			<object type="Zutaten" xml:id="semmeln-eingeweicht">Semmeln (eingeweicht)</object>
