			<object type="MaterielleKultur" xml:id="ofen">Ofen</object>
			<object type="MaterielleKultur" xml:id="raedlein">Rädlein</object>
			<object type="MaterielleKultur" xml:id="seitlein">Seitlein</object>
			<object type="MaterielleKultur" xml:id="tiegel-irden">Tiegel (irden)</object>
