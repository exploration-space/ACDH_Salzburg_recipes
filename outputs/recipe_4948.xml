             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="kohlen">Kohlen</object>
			<object type="MaterielleKultur" xml:id="loeffel">Löffel</object>
			<object type="MaterielleKultur" xml:id="stollhafen">Stollhafen</object>
			<object type="MaterielleKultur" xml:id="stuerze-deckel">Stürze (Deckel)</object>
