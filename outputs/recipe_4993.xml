        <body>
            <div type="ingredient">
                <listObject>
			<object type="Zutaten" xml:id="kuemmel">Kümmel</object>
			<object type="Zutaten" xml:id="pfeffer">Pfeffer</object>
			<object type="Zutaten" xml:id="pfifferling-eierschwammerl">Pfifferling (Eierschwammerl)</object>
			<object type="Zutaten" xml:id="salz">Salz</object>
