        <body>
            <div type="ingredients">
                <listObject>
			<object type="Zutaten" xml:id="eiklar">Eiklar (Eiweiß)</object>
			<object type="Zutaten" xml:id="roggenmehl">Roggenmehl</object>
			<object type="Zutaten" xml:id="salz">Salz</object>
