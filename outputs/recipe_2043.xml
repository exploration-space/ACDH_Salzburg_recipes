             <div type="utensil">
                <listObject>
			<object type="MaterielleKultur" xml:id="waffel-eisen">(Waffel-)Eisen</object>
			<object type="MaterielleKultur" xml:id="lederstueck">Lederstück</object>
			<object type="MaterielleKultur" xml:id="messer">Messer</object>
			<object type="MaterielleKultur" xml:id="nudelwalker-nudelholz">Nudelwalker (Nudelholz)</object>
