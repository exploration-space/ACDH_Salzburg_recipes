             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="glut">Glut</object>
			<object type="MaterielleKultur" xml:id="rost">Rost</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="tiegel">Tiegel</object>
                </listObject>
