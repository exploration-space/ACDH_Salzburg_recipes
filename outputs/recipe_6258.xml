             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="feuer">Feuer</object>
			<object type="MaterielleKultur" xml:id="leintuch">Leintuch</object>
			<object type="MaterielleKultur" xml:id="moerser">Mörser</object>
			<object type="MaterielleKultur" xml:id="sieb">Sieb</object>
			<object type="MaterielleKultur" xml:id="tiegel">Tiegel</object>
			<object type="MaterielleKultur" xml:id="hafen-topf">Topf (Hafen)</object>
