			<object type="MaterielleKultur" xml:id="deckel">Deckel</object>
			<object type="MaterielleKultur" xml:id="glas">Glas</object>
			<object type="MaterielleKultur" xml:id="kessel">Kessel</object>
			<object type="MaterielleKultur" xml:id="kessel-verzinnt">Kessel (verzinnt)</object>
			<object type="MaterielleKultur" xml:id="pfanne">Pfanne</object>
			<object type="MaterielleKultur" xml:id="stecknadel">Stecknadel</object>
