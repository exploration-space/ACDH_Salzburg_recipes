			<object type="Zutaten" xml:id="petersilienwurzel">Petersilienwurzel</object>
			<object type="Zutaten" xml:id="roehrenwasser-leitungswasser">Röhrenwasser (Leitungswasser)</object>
			<object type="Zutaten" xml:id="safran">Safran</object>
			<object type="Zutaten" xml:id="salz">Salz</object>
                 </listObject>
            </div>
             <div type="utensils">
                <listObject>
			<object type="MaterielleKultur" xml:id="pfanne">Pfanne</object>
			<object type="MaterielleKultur" xml:id="schuessel">Schüssel</object>
			<object type="MaterielleKultur" xml:id="hafen-topf">Topf (Hafen)</object>
			<object type="MaterielleKultur" xml:id="topf">Topf (Rein)</object>
                </listObject>
